container_commands:
  01_migrate:
    command: "django-admin.py migrate"
    leader_only: true
  02_createsu:
    command: "django-admin.py createsu"
    leader_only: true
  03_collectstatic:
    command: "python manage.py collectstatic --noinput"

option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "core.settings"